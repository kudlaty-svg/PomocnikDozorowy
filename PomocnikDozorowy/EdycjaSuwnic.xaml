<Window x:Class="PomocnikDozorowy.EdycjaSuwnic"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PomocnikDozorowy"
        mc:Ignorable="d"
        Title="Edycja Suwnic" Height="450" Width="500" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <ComboBox x:Name="ID_suwnic" HorizontalAlignment="Left" Margin="180,50,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Wybierz ID urzadzenia do edycji" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" Height="25"/>
        <Button x:Name="Btn_Zapisz" Content="Zapisz" HorizontalAlignment="Left" Margin="180,364,0,0" VerticalAlignment="Top" IsEnabled="False" Width="50" Height="22"/>
        <Button x:Name="Btn_Anuluj" Content="Anuluj" HorizontalAlignment="Left" Margin="250,364,0,0" VerticalAlignment="Top" Click="Btn_Anuluj_Click" Width="50" Height="22"/>
        <Label Content="Stare Dane" HorizontalAlignment="Left" Margin="20,85,0,0" VerticalAlignment="Top"/>
        <Label Content="Nowe Dane" HorizontalAlignment="Left" Margin="250,85,0,0" VerticalAlignment="Top"/>
        <Label Content="ID Dźwignicy" HorizontalAlignment="Left" Margin="35,110,0,0" VerticalAlignment="Top"/>
        <Label Content="ID Dźwignicy" HorizontalAlignment="Left" Margin="265,110,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="Stare_ID" HorizontalAlignment="Left" Margin="120,116,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Nowe_ID" HorizontalAlignment="Left" Margin="350,116,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>


    </Grid>
</Window>
